<h1>Artist: <%= @artist.name %></h1>
<%= image_tag @artist.image_path %>
<%= link_to "Edit", edit_artist_path(@artist.id) %>
<%= link_to "Delete", artist_path(@artist.id), method: :delete %>
<%= link_to "New Song", new_artist_song_path(@artist, @song) %>
<%= link_to "View Songs", artist_songs_path(@artist)  %>
<p><%= link_to "<< Back to Artists", artists_path %></p>
